{
  "name": "@authing/guard-angular",
  "version": "5.3.4",
  "description": "Guard for Angular",
  "module": "dist/fesm2015/authing-guard-angular.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "autoinstall": "npm ci",
    "build": "npm run build:ng && npm run build:assets",
    "build:assets": "node scripts/build.js",
    "build:ng": "ng build guard-angular",
    "lint": "eslint --ext .ts src/**",
    "lint:fix": "eslint --fix --ext .ts src/**",
    "release:official": "npm publish --verbose --access public",
    "release:alpha": "npm publish --verbose --tag=alpha --access public"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.1",
    "@angular/cli": "^14.1.1",
    "css-loader": "^6.7.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "ng-packagr": "^14.1.0",
    "rimraf": "^2.6.2",
    "ts-loader": "^9.3.1",
    "webpack": "^5.72.0"
  },
  "peerDependencies": {
    "@angular/core": "^14.0.0"
  },
  "dependencies": {
    "@authing/guard": "^5.3.4"
  },
  "author": "https://github.com/authing",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
