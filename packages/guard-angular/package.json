  "license": "MIT",
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.1",
    "@angular/cli": "^14.1.1",
    "ng-packagr": "^14.1.0"
  },
  "peerDependencies": {
    "@angular/core": "^14.0.0"
  },
  "dependencies": {
    "@authing/guard": "^6.0.0-alpha.12"
  }
}
