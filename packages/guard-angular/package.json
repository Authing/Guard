{
  "name": "@authing/guard-angular",
  "version": "6.0.0-alpha.12",
  "description": "Authing Guard for Angular",
  "module": "dist/fesm2015/authing-guard-angular.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "autoinstall": "npm ci",
    "build:lib": "npm run build:ng && npm run build:assets",
    "build:assets": "node scripts/build.js",
